#include <iostream>
#include <queue>
using namespace std;

class Stack {
    queue<int> q1, q2;
public:
    void push(int x) {
        q2.push(x);
        while(!q1.empty()) {
            q2.push(q1.front()); q1.pop();
        }
        swap(q1, q2);
    }
    int pop() {
        if(q1.empty()) return -1;
        int val = q1.front(); q1.pop();
        return val;
    }
    int top() {
        return q1.empty() ? -1 : q1.front();
    }
    bool empty() {
        return q1.empty();
    }
};

int main() {
    Stack s;
    s.push(5); s.push(7); s.push(9);
    cout << s.pop() << endl; // 9
    cout << s.top() << endl; // 7
    s.push(3);
    cout << s.pop() << endl; // 3
    return 0;
}
