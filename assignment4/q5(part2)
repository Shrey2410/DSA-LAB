#include <iostream>
#include <queue>
using namespace std;

class Stack {
    queue<int> q;
public:
    void push(int x) {
        q.push(x);
        int sz = q.size();
        // Rotate all except the newly added element to the back
        for(int i = 0; i < sz-1; ++i) {
            q.push(q.front());
            q.pop();
        }
    }
    int pop() {
        if(q.empty()) return -1; // Stack underflow
        int val = q.front();
        q.pop();
        return val;
    }
    int top() {
        if(q.empty()) return -1;
        return q.front();
    }
    bool empty() {
        return q.empty();
    }
};

int main() {
    Stack s;
    s.push(10);
    s.push(20);
    s.push(30);
    cout << s.pop() << endl; // 30
    cout << s.top() << endl; // 20
    s.push(40);
    cout << s.pop() << endl; // 40
    cout << s.pop() << endl; // 20
    cout << s.pop() << endl; // 10
    cout << s.pop() << endl; // -1 (empty stack)
    return 0;
}
