#include <iostream>
#include <queue>
using namespace std;

int main() {
    queue<int> q1, q2;
    int arr[] = {4, 7, 11, 20, 5, 9}, n = 6;
    for(int i = 0; i < n; i++) q1.push(arr[i]);
    for(int i = 0; i < n/2; i++) q2.push(q1.front()), q1.pop();
    while(!q2.empty() && !q1.empty()) {
        cout << q2.front() << " "; q2.pop();
        cout << q1.front() << " "; q1.pop();
    }
    cout << endl;
    return 0;
}
