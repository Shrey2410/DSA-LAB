#include <iostream>
#include <queue>
using namespace std;

int main() {
    queue<int> q;
    int choice, val;
    do {
        cout << "1.Enqueue 2.Dequeue 3.Peek 4.Display 5.Exit : "; cin >> choice;
        switch(choice) {
            case 1: cout << "Enter value: "; cin >> val; q.push(val); break;
            case 2: 
                if(!q.empty()) { cout << "Dequeued: " << q.front() << endl; q.pop(); }
                else cout << "Queue empty!\n";
                break;
            case 3: 
                if(!q.empty()) cout << "Front: " << q.front() << endl;
                else cout << "Queue empty!\n";
                break;
            case 4: 
                if(q.empty()) cout << "Queue empty!\n";
                else { queue<int> temp = q;
                    while(!temp.empty()) { cout << temp.front() << " "; temp.pop(); }
                    cout << endl;
                }
                break;
        }
    } while(choice != 5);
    return 0;
}
